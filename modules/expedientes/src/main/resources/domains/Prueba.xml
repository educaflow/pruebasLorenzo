<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="expedientes" package="com.educaflow.apps.expedientes.db"/>
    <entity name="Prueba" extends="Expediente" >
        <string name="solicita" />
        <string name="motivo" />
        <binary name="justificante" />
        <string name="justificanteFileName" />
        <string name="justificanteFileType" />
        <one-to-many name="formacionesCentrosTrabajo" ref="FormacionCentroTrabajo" mappedBy="expediente" />
        <extra-code-model><![CDATA[
public enum Estado {
    ENTRADA_DATOS,
    REVISION,
    ACEPTADO,
    RECHAZADO;
}

public enum Evento {
    PRESENTAR,
    SUBSANAR,
    ACEPTAR,
    RECHAZAR;
}

public void changeState(Estado estado) {
    super.changeState(estado);
}
		]]></extra-code-model>



    </entity>

</domain-models>