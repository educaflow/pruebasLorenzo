<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="expedientes" package="com.educaflow.apps.expedientes.db"/>
    <entity name="ExpedienteHistorialEstados">
        <string name="codeState" title="Código estado"/>
        <string name="nameState" title="Estado" translatable="true"/>
        <string name="codeEvent" title="Código Evento" />
        <string name="nameEvent" title="Evento"/>
        <one-to-one name="expediente" ref="Expediente" />
        <datetime name="fecha" />
    </entity>

</domain-models>