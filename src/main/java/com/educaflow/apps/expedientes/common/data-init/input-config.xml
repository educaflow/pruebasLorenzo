<?xml version="1.0"?>
<xml-inputs priority="10" xmlns="http://axelor.com/xml/ns/data-import"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://axelor.com/xml/ns/data-import
  https://axelor.com/xml/ns/data-import/data-import_7.4.xsd">




    <input file="TipoTramites.xml" root="datos">

        <bind node="tipostramites/tipotramite" type="com.educaflow.apps.expedientes.db.TipoTramite" search="self.code = :code"  create="true" update="true">
            <bind node="@code" to="code" />
            <bind node="@name" to="name" />
        </bind>


    </input>


</xml-inputs>