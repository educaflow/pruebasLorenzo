<?xml version="1.0"?>
<xml-inputs priority="-1" xmlns="http://axelor.com/xml/ns/data-import"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://axelor.com/xml/ns/data-import
  https://axelor.com/xml/ns/data-import/data-import_7.4.xsd">




    <input file="TiposExpedientesActivos.xml" root="datos">

        <bind node="tramites/tramite" type="com.educaflow.apps.expedientes.db.Tramite" search="self.code = :code" update="true" >
            <bind node="@tipoExpediente" to="tipoExpediente" alias="code" search="self.code = :code"  create="false"  />
        </bind>
    </input>


</xml-inputs>