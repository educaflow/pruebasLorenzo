<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="sistemaeducativo" package="com.educaflow.apps.sistemaeducativo.db"/>
    <entity name="CursoModulo">
        <many-to-one name="curso"  ref="com.educaflow.apps.sistemaeducativo.db.Curso"   required="true"  />
        <many-to-one name="modulo" ref="com.educaflow.apps.sistemaeducativo.db.Modulo"  required="true"  />

        <unique-constraint columns="curso,modulo" />

    </entity>

</domain-models>