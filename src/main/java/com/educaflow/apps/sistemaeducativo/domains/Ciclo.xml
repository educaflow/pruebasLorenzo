<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models https://axelor.com/xml/ns/domain-models/domain-models_7.4.xsd">

    <module name="sistemaeducativo" package="com.educaflow.apps.sistemaeducativo.db"/>
    <entity name="Ciclo">
        <string name="code"  required="true"  />
        <string name="name" namecolumn="true"  required="true"  />
        <one-to-many name="cursos" ref="com.educaflow.apps.sistemaeducativo.db.Curso" mappedBy="ciclo" />
        <many-to-one name="familiaProfesional" ref="com.educaflow.apps.sistemaeducativo.db.FamiliaProfesional"  required="true"  />
        <many-to-one name="grado" ref="com.educaflow.apps.sistemaeducativo.db.Grado"  required="true"  />
    </entity>

</domain-models>